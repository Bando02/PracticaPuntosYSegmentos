package clases;

public class Punto {
	
	
	 
	int x;
	int y;
	
	
	/**
	 * Constructor default agrega valores a X e Y
	 * 
	 * 
	 */
	public Punto() {
		this.x = 0;
		this.y = 0;
	}
	
	/**
	 * Constructor que crea objeto con los parametros pasados "X" e "Y"
	 * 
	 * @param x
	 * @param y
	 */
	public Punto(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	
	public void setX(int x) {
        this.x = x;
    }
	
	public void setY(int y) {
		this.y = y;
	}

}
