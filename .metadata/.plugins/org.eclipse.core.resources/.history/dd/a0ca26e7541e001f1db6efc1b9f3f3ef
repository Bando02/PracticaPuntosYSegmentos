package clases;

public class Segmento {
	
	Punto a;
	Punto b;
	
	public Segmento() {
			
		a = new Punto();
		b = new Punto(1,1);
	}
	
	public Segmento(Punto a, Punto b) {
		this.a = a;
		this.b = b;
	}
	
	public Punto getA() {
		return a;
	}
	public Punto getB() {
		return b;
	}
	public void setA(Punto a) {
		this.a = a;
	}
	public void setB(Punto b) {
		this.b = b;
	}
	

}
